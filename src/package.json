{
  "name": "novo-site-womakerscode",
  "version": "1.0.0",
  "description": "Projeto do novo site do WoMakersCode feito em Node.js, Jekill & HTML",
  "main": "index.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint *.js",
    "fix": "node_modules/.bin/eslint *.js --fix",
    "prepush": "npm run lint",
    "build-pages": "node ./build_scripts/builder.js",
    "start": "concurrently --kill-others \"nodemon -e js,json,css,md -i build -x \\\"npm run build-pages\\\"\" \"live-server ./build\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WoMakersCode/networking.git"
  },
  "keywords": [
    "static",
    "page",
    "generator",
    "html",
    "build",
    "start",
    "nodejs",
    "jekill",
    "html",
    "css",
    "server"
  ],
  "author": "Cynthia Zanoni (@cyn), Glaucia Lemos (@glaucia86), Luciana Marques (@LucianaMarques)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WoMakersCode/networking/issues"
  },
  "homepage": "https://github.com/WoMakersCode/networking/blob/master/README.md",
  "dependencies": {
    "concurrently": "^3.5.1",
    "fs-extra": "^4.0.2",
    "live-server": "^1.2.1",
    "markdown-it": "^8.4.0",
    "nodemon": "^1.18.9"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.3.1"
  }
}
